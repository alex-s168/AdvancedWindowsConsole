echo.
goto input

:apps
echo.
set cmd=
set /a c=0
for /f "tokens=*" %%a in (%path_code%APPS.cfg) do (
	set variable=%%a
	set /a c=c+1
	FOR /F "tokens=1-2 delims=#" %%G IN ("!variable!") DO (
	
		echo APP/COMMAND:  %%G               %%H
	
	)
	
	set variable=
)
echo.
goto input

:config
REM ║╔╗╦╬╠╣═╚╝
if NOT /f %arg1%=="" (
	echo ╔════════════════════════════════════════════════════════════════════════════════════
	echo ║                  Current settings:                
	echo ╠════════════════════════════════════════════════════════════════════════════════════
